<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <TextView
        android:id="@+id/mytextView6"
        android:layout_width="141dp"
        android:layout_height="58dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="48dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mytextView5" />

    <TextView
        android:id="@+id/mytextview"
        android:layout_width="162dp"
        android:layout_height="59dp"
        android:layout_marginTop="180dp"
        android:text=""
        android:textColor="@android:color/holo_orange_light"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.232"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/myedittext"
        android:layout_width="168dp"
        android:layout_height="70dp"
        android:layout_marginTop="48dp"
        android:ems="10"
        android:hint="number"
        android:inputType="numberDecimal"
        android:text=""
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.883"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/mytextView2"
        android:layout_width="161dp"
        android:textColor="@android:color/holo_orange_light"
        android:layout_height="56dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="188dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mytextview" />

    <TextView
        android:id="@+id/mytextView3"
        android:layout_width="160dp"
        android:layout_height="60dp"
        android:textColor="@android:color/holo_orange_light"
        android:layout_marginTop="56dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.247"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mytextView2" />

    <TextView
        android:id="@+id/mytextView4"
        android:layout_width="141dp"
        android:layout_height="66dp"
        android:layout_marginTop="180dp"
        android:layout_marginEnd="48dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/mytextView5"
        android:layout_width="144dp"
        android:layout_height="60dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="40dp"
        app:layout_constraintEnd_toStartOf="@+id/mytextView2"
        app:layout_constraintHorizontal_bias="0.352"
        app:layout_constraintStart_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mytextView4" />

    <ImageButton
        android:id="@+id/mybutton"
        android:layout_width="114dp"
        android:layout_height="89dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="88dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/picture1" />

    <ImageButton
        android:id="@+id/mybutton2"
        android:layout_width="108dp"
        android:layout_height="88dp"
        android:layout_marginEnd="172dp"
        android:layout_marginBottom="88dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/picture2" />

    <ImageButton
        android:id="@+id/mybutton3"
        android:layout_width="127dp"
        android:layout_height="87dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="88dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/mybutton2"
        app:srcCompat="@drawable/picture3" />

    <Spinner
        android:id="@+id/myspinner"
        android:layout_width="164dp"
        android:layout_height="73dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="48dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>